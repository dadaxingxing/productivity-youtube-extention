{
  "manifest_version": 3,
  "name": "Lock'd",
  "version": "1.0",
  "description": "Blurs all YouTube thumbnails and omit titles except whitelisted channels",
  "content_scripts": [
    {
      "matches": ["*://www.youtube.com/*"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ],
  "permissions": ["activeTab"],

  "action" : {
    "default_popup": "popup.html"
  }
}
